{
  "name": "@amap-devkit/core",
  "version": "0.0.4",
  "description": "@amap-devkit/core",
  "main": "index.js",
  "module": "dist/core.es.js",
  "types": "dist/core.d.ts",
  "files": [
    "index.js",
    "dist",
    "compiler"
  ],
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "node": {
        "production": "./dist/core.cjs.prod.js",
        "development": "./dist/core.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/core.es.js",
      "import": "./dist/core.es.js",
      "require": "./index.js"
    },
    "./compiler": {
      "types": "./compiler/index.d.ts",
      "import": "./compiler/index.mjs",
      "require": "./compiler/index.js"
    }
  },
  "buildOptions": {
    "name": "AmapDevkitCore",
    "formats": [
      "es",
      "cjs",
      "umd"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitterBar/amap-devkit",
    "directory": "packages/vue"
  },
  "keywords": [
    "amap-devkit"
  ],
  "author": "Bitter Bar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitterBar/amap-devkit/issues"
  },
  "homepage": "https://github.com/BitterBar/amap-devkit/tree/main/packages/core#readme",
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.15"
  }
}
